<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.docentes"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.docentes.Main_Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.docentes.MenuPrincipal"
            android:label="@string/title_activity_menu_principal" >
        </activity>
        <activity
            android:name="com.example.docentes.Registrar"
            android:label="@string/title_activity_registrar" >
        </activity>
        <activity
            android:name="com.example.docentes.ListarDocentes"
            android:label="@string/title_activity_listar_docentes" >
        </activity>
        <activity
            android:name="com.example.docentes.Buscar"
            android:label="@string/title_activity_buscar" >
        </activity>
        <activity
            android:name="com.example.docentes.Editar"
            android:label="@string/title_activity_editar" >
        </activity>
        <activity
            android:name="com.example.docentes.Eliminar"
            android:label="@string/title_activity_eliminar" >
        </activity>
    </application>

</manifest>
